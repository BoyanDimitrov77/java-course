package Person;

public class PrintInfo {

	public static void main(String[] args) {
		
		Teacher teachers=new Teacher("Iva","Dobreva",5,"History");
		System.out.println("Name:"+teachers.getFirst_name());
		System.out.println("Last name:"+teachers.getLast_name());
		System.out.println("Experience:"+teachers.getExperience() + "years");
		System.out.println("Subject:"+teachers.getNameSubject());
		
		teachers.SayGoodDay();
		teachers.SayGoodBye();
		teachers.checkHomework();
		
		Lecturer lecturers=new Lecturer("Stoqn","Petrov",10,"University");
		System.out.println("Name:"+lecturers.getFirst_name());
		System.out.println("Last name:"+lecturers.getLast_name());
		System.out.println("Experience:"+lecturers.getExperience() + "years");
		System.out.println("Subject:"+lecturers.getName_uni());
		
		lecturers.doExcercises("Monday");
		lecturers.doExcercises("Tuesday","Friday");
		lecturers.doExcercises("Monday", "Wednesday", "Saturday");
		lecturers.SayGoodDay();
		
		Proffesor proffesors=new Proffesor("Ivan","Ivanov",20,"University","Deegree");
		System.out.println("Name:"+proffesors.getFirst_name());
		System.out.println("Last name:"+proffesors.getLast_name());
		//System.out.println("Experience:"+proffesors.getExperience() + "years");
		System.out.println(proffesors.doPirnt());
		System.out.println("Subject:"+proffesors.getName_uni());
		
		
		
		
		

	}

}
