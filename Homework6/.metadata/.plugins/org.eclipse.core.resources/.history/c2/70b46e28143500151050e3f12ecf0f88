package Employee;

public class Inheritance {

	public static void main(String[] args) {
		Policeman person1= new Policeman(80,2000,"commissar");
		person1.sayRang();
		person1.getWork_Hours();
		person1.getSalary();
		
		Doctor person2=new Doctor(100,2000,12,4);
		person2.printDuty();
		
		person2.getWork_Hours();
		person2.getSalary();

	}

}
