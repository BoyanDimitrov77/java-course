package BankAccount;



public class Account {

	protected double sum_bank=0;

	

	public Account(double sum_bank){
		this.sum_bank=sum_bank;
		if(sum_bank<0){
			throw new IllegalArgumentException("Sum cannot be negative!");
		}
	}
	
	public double getSum_bank() {
		return sum_bank;
	}

	public void setSum_bank(double sum_bank) {
		this.sum_bank = sum_bank;
		
		
	}

	
		
		
	}


