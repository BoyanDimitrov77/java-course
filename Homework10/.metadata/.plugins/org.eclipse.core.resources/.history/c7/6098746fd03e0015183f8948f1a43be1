package Cards;



public class Card {
private String face;
private Paint paint;
	
public Card(String face){
	this.face=face;
	//this.paint=paint;

}	

public String getFace() {
	return face;
}

public void setFace(String face) {
	this.face = face;
}

public Paint getPaint() {
	return paint;
}

public void setPaint(Paint paint) {
	this.paint = paint;
}


enum Paint{
	CLUB, DIAMOND, HEART, SPADE
}

@Override
public String toString(){
	return String.format("%s %s",this.face,this.paint);
	
}


}
